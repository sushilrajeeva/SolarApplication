    <!-- Page Header -->
    <header  >
      <div class="overlay"></div>
      <div class="container">
        <div class="row">
          <div class="col-lg-8 col-md-10 mx-auto">
            <div class="page-heading">
              <h1 style="text-align: center;">{{title}}</h1>
            </div>
          </div>
        </div>
      </div>
    </header>

    <div>
        {{{success}}}
        
    </div>

    <!-- Main Content -->
    <div style="display: flex; justify-content: center; align-items: center;">

        <form class="login-form" method="post" id="login-form">
        <label for="userTypeInput">Select User Type:</label> <br>
                <select id="userTypeInput" name="userTypeInput" required>
                    <option value="normal user" selected>Normal User</option>
                    <option value="sales">Sales</option>
                    <option value="management">Management</option>
                    <option value="crew">Crew</option>
                </select> <br>
        <label for="emailAddressInput">Email Address: </label><br>
        <input type="email" id="emailAddressInput" name="emailAddressInput" placeholder="Please enter your email address"><br>
        <label for="passwordInput">Password: </label><br>
        <input type="password" id="passwordInput" name="passwordInput" placeholder="Please enter your password"><br>
        <input type="submit" value="Submit">
        <p>
            Don't have an account? <a href="/signup"> Click here to register</a>
        </p>
    </form> 

        
        
    </div>

    <div>
        <div id="error" class="error" hidden></div>
        {{{error}}}
    </div>

    

